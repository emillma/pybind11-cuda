{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "/usr/bin/python3.10",
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "preLaunchTask": "make",
            "args": [
                "${workspaceFolder}/test_mul.py"
            ],
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "CUDA C++: Launch",
            "type": "cuda-gdb",
            "request": "launch",
            "program": "/usr/bin/python3.10",
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "preLaunchTask": "make",
            "args": "${workspaceFolder}/test_mul.py",
            "env": {
                "CUDA_DEBUGGER_SOFTWARE_PREEMPTION": "1"
            },
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
        },
        {
            "name": "Python: Ins",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/ins/run.py",
            "console": "integratedTerminal",
            "justMyCode": true,
        },
    ]
}